FROM us.gcr.io/platform-205701/jenkins-slave-portal-open-8u242:latest
RUN curl -OL https://github.com/adoptium/temurin11-binaries/releases/download/jdk-11.0.14%2B9/OpenJDK11U-jdk_x64_linux_hotspot_11.0.14_9.tar.gz
RUN tar -xzf OpenJDK11U-jdk_x64_linux_hotspot_11.0.14_9.tar.gz
ENV JAVA_HOME jdk-11.0.14+9
ENV PATH $JAVA_HOME/bin:/usr/local/src/apache-maven/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/opt/gsutil
RUN java -version